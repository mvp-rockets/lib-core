{
  "name": "@mvp-rockets/namma-lib",
  "version": "2.0.6",
  "description": "A functional utility library for developing express api",
  "main": "index.js",
  "scripts": {
    "start": ". ~/.nvm/nvm.sh && nvm use; nodemon index.js",
    "test:watch": "NODE_ENV=test ./node_modules/.bin/supervisor -q -n exit -x ./node_modules/.bin/mocha -- --recursive --config test/.mocharc.js",
    "test:ci": "NODE_ENV=ci ./node_modules/.bin/mocha --recursive --exit --config ./test/.mocharc.js",
    "coverage": "NODE_ENV=test nyc --reporter=html ./node_modules/.bin/mocha --recursive --config ./test/.mocharc.js --exit"
  },
  "engines": {
    "node": ">=16.x"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mvp-rockets/lib-core"
  },
  "keywords": [
    "functional",
    "helper",
    "ramda",
    "express"
  ],
  "publishConfig": {
    "provenance": true
  },
  "author": "Napses.com",
  "license": "MIT",
  "dependencies": {
    "@google-cloud/logging-winston": "^6.0.0",
    "@serdnam/pino-cloudwatch-transport": "^1.0.4",
    "app-module-path": "^2.2.0",
    "aws-sdk": "^2.1576.0",
    "axios": "^1.6.7",
    "axios-retry": "^4.0.0",
    "bottleneck": "^2.19.5",
    "cls-hooked": "^4.2.2",
    "file-stream-rotator": "^1.0.0",
    "folktale": "^2.3.2",
    "google-libphonenumber": "^3.2.34",
    "jsonwebtoken": "^9.0.2",
    "nodemon": "^3.1.0",
    "nyc": "^15.1.0",
    "pino": "^8.19.0",
    "pino-loki": "^2.1.3",
    "ramda": "^0.29.1",
    "supervisor": "^0.12.0",
    "uuid": "^9.0.1",
    "uuid-validate": "^0.0.3",
    "winston": "^3.12.0",
    "winston-cloudwatch": "^6.2.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@babel/eslint-parser": "^7.23.10",
    "chai": "^4.4.1",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "factory-girl": "^5.0.4",
    "mocha": "^10.3.0",
    "sinon": "^17.0.1",
    "sinon-chai": "^3.7.0"
